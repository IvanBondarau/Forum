{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "uikit@3.5.7",
      "destination": "wwwroot/lib/uikit/"
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "signalr.js@2.4.1",
      "destination": "wwwroot/lib/signalr.js/"
    },
    {
      "library": "aspnet-signalr@1.1.4",
      "destination": "wwwroot/lib/aspnet-signalr/"
    },
    {
      "library": "microsoft-signalr@5.0.0",
      "destination": "wwwroot/lib/microsoft-signalr/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}